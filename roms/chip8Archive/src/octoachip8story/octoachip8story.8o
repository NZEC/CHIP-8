#------------------------------------------------------------------------------#
: main
	loop
		if game_screen == 0 then title
		if game_screen == 1 then pic_5
		if game_screen == 2 then pic_6
		if game_screen == 3 then pic_7	
		if game_screen == 4 then pic_8
		if game_screen == 5 then pic_9	
		if game_screen == 6 then pic_10	
		if game_screen == 7 then pic_12	
		if game_screen == 8 then pic_13		
		if game_screen == 9 then pic_14	
		if game_screen == 10 then pic_15
		if game_screen == 11 then pic_16			
		again
	return 

#------------------------------------------------------------------------------#
