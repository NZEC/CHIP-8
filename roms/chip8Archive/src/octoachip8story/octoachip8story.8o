#------------------------------------------------------------------------------#

: title
	i := sx_title
	draw_fullscreen
	i := sx_star
	sprx := 1  # x

	if ending1 == 1 begin
	spry := 1   # y 
	sprite sprx spry 3
	end

	if ending2 == 1 begin
	spry := 5   # y 
	sprite sprx spry 3
	end	

	if ending3 == 1 begin
	spry := 9   # y 
	sprite sprx spry 3
	end

	if ending4 == 1 begin
	spry := 13   # y 
	sprite sprx spry 3
	end

	if ending5 == 1 begin
	spry := 17   # y 
	sprite sprx spry 3
	end
	loop 
		keypressed := key # Did they press ANYTHING?
		if keypressed >= 0 begin
			game_screen := 1
			clear
			return
		end
		again	

